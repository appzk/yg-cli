{
  "name": "yg-cli",
  "version": "1.0.0",
  "description": "A simple CLI for front-end engineering automation construction tool.",
  "preferGlobal": true,
  "bin": {
    "yg": "bin/yg",
    "yg-init": "bin/yg-init",
    "yg-list": "bin/yg-list"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appzk/yg-cli.git"
  },
  "keywords": [
    "yg",
    "cli",
    "spa"
  ],
  "author": "appzk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appzk/yg-cli/issues"
  },
  "homepage": "https://github.com/appzk/yg-cli#readme",
  "scripts": {
    "test": "npm run unit && npm run lint && npm run e2e",
    "lint": "eslint test/e2e/test*.js lib bin/* --env",
    "e2e": "rimraf test/e2e/mock-template-build --slow 1000",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "commit": "git-cz"
  },
  "dependencies": {
    "async": "^3.1.0",
    "chalk": "^3.0.0",
    "coffeescript": "^2.4.1",
    "commander": "^4.0.1",
    "consolidate": "^0.15.1",
    "download-git-repo": "^3.0.2",
    "handlebars": "^4.5.3",
    "inquirer": "^7.0.0",
    "metalsmith": "^2.3.0",
    "minimatch": "^3.0.4",
    "multimatch": "^4.0.0",
    "ora": "^4.0.3",
    "read-metadata": "^1.0.0",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "semver": "^6.3.0",
    "tildify": "^2.0.0",
    "uid": "^0.0.2",
    "user-home": "^2.0.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.0",
    "@testing-library/user-event": "^7.1.2",
    "alex": "^8.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "chai": "^4.2.0",
    "chromedriver": "^78.0.1",
    "cz-conventional-changelog": "^3.2.0",
    "debug": "^4.1.1",
    "eslint": "^6.6.0",
    "execa": "^3.3.0",
    "fs-extra": "^8.1.0",
    "get-port": "^5.0.0",
    "globby": "^9.1.0",
    "husky": "^1.3.1",
    "jest": "24.9.0",
    "lerna": "3.16.4",
    "lerna-changelog": "~0.8.2",
    "lint-staged": "^8.0.4",
    "meow": "^5.0.0",
    "prettier": "1.18.2",
    "puppeteer": "^1.13.0",
    "strip-ansi": "^5.1.0",
    "svg-term-cli": "^2.1.1",
    "tempy": "^0.2.1",
    "wait-for-localhost": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css,html}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  }
}
